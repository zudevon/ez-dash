{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "NEWS_API_KEY": "@news_api_key",
    "WEATHER_API_KEY": "@weather_api_key",
    "TIINGO_API_KEY": "@tiingo_api_key"
  }
}
